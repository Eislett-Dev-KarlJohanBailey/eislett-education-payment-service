{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "package": {
      "dependsOn": ["build"],
      "outputs": ["function.zip"]
    },
    "deploy": {
      "dependsOn": ["package"],
      "outputs": []
    }
  }
}